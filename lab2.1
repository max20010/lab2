#include "pch.h"
#include <iostream>
#include <math.h>
using namespace std;

int main()
{
	double x1, x2, y1, y2, x, y, c;
	cin >> x1;
	cin >> y1;
	cin >> x2;
	cin >> y2;
	if (x1 > x2) {
		x = x1 - x2;
	}
	else
	{
		x = x2 - x1;
	}	if (y1 > y2) {
		y = y1 - y2;
	}
	else
	{
		y = y2 - y1;
	}
	c = y * y + x * x;
	c = sqrt(c);
	cout << c << endl;
}
